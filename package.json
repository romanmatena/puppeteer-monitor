  "bin": {
    "browsermonitor": "./src/cli.mjs"
  },
  "main": "./src/monitor.mjs",
  "files": [
    "src"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/romanmatena/browsermonitor.git"
  },
  "author": "24gate",
  "license": "MIT",
  "dependencies": {
    "boxen": "^8.0.1",
    "cli-table3": "^0.6.5",
    "puppeteer": "^24.36.1"
  },
  "puppeteer": {
    "skipDownload": true
  },
  "packageManager": "pnpm@10.25.0+sha512.5e82639027af37cf832061bcc6d639c219634488e0f2baebe785028a793de7b525ffcd3f7ff574f5e9860654e098fe852ba8ac5dd5cefe1767d23a020a92f501",
  "homepage": "https://github.com/romanmatena/browsermonitor#readme",
  "bugs": {
    "url": "https://github.com/romanmatena/browsermonitor/issues"
  },
  "keywords": [
    "browser",
    "browsermonitor",
    "chrome",
    "console",
    "debugging",
    "devtools",
    "dom",
    "llm",
    "monitoring",
    "network",
    "puppeteer",
    "screenshot",
    "wsl"
  ],
  "publishConfig": {
    "access": "public"
  }
